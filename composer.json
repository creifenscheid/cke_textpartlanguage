{
	"name" : "creifenscheid/cke-textpartlanguage",
	"type" : "typo3-cms-extension",
	"description" : "This TYPO3 extension implements the text part language feature for CKEditor 5.",
	"license" : "GPL-2.0-or-later",
	"require" : {
		"typo3/cms-core" : "^12.4",
		"typo3/cms-rte-ckeditor" : "^12.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"replace": {
		"typo3-ter/cke-textpartlanguage": "self.version"
	},
	"minimum-stability" : "stable",
	"authors" : [{
		"name" : "Christian Reifenscheid",
		"email" : "dev@christian-reifenscheid.de"
	}
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "cke_textpartlanguage"
		}
	}
}
